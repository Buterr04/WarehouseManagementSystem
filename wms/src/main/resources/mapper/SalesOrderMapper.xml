<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.dai.wms.mapper.SalesOrderMapper">

    <resultMap id="BaseResultMap" type="com.dai.wms.entity.SalesOrder">
        <id column="sales_order_id" property="salesOrderId" />
        <result column="customer_id" property="customerId" />
        <result column="sale_date" property="saleDate" />
    </resultMap>

    <sql id="Base_Column_List">
        sales_order_id, customer_id, sale_date
    </sql>

    <insert id="insert" parameterType="com.dai.wms.entity.SalesOrder" useGeneratedKeys="true" keyProperty="salesOrderId">
        insert into sales_order (customer_id, sale_date)
        values (#{customerId}, #{saleDate})
    </insert>

</mapper>